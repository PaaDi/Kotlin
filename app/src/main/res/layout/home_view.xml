<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <fragment
        android:id="@+id/menuFragment"
        android:name="com.madera.kotlin.Controller.Menu.MenuFragment"
        android:layout_width="907dp"
        android:layout_height="135dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <fragment
        android:id="@+id/filterfragment"
        android:name="com.madera.kotlin.Controller.Menu.FilterFragment"
        android:layout_width="900dp"
        android:layout_height="79dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuFragment"
        tools:ignore="MissingConstraints" />

    <fragment
        android:id="@+id/buttonFragment"
        android:name="com.madera.kotlin.Controller.Menu.ButtonFragment"
        android:layout_width="900dp"
        android:layout_height="220dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filterfragment" />

    <fragment
        android:id="@+id/fragment3"
        android:name="com.madera.kotlin.Controller.Menu.ProjectListFragment"
        android:layout_width="900dp"
        android:layout_height="761dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonFragment" />



</androidx.constraintlayout.widget.ConstraintLayout>